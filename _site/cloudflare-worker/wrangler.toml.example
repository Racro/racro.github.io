# Cloudflare Worker Configuration
# Copy this file to wrangler.toml and fill in your values

name = "comments-worker"
main = "comments.js"
compatibility_date = "2024-01-01"

# KV Namespace binding
# Get your namespace ID from Cloudflare Dashboard → Workers & Pages → KV
[[kv_namespaces]]
binding = "COMMENTS_KV"
id = "YOUR_NAMESPACE_ID_HERE"  # Replace with your actual KV namespace ID

